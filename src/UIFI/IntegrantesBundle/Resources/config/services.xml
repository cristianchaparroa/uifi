<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
      <service class="UIFI\IntegrantesBundle\Service\IntegrantesService" id="uifi.integrantes.integrantes">
          <argument type="service" id="service_container" />
      </service>
      <service class="UIFI\IntegrantesBundle\Service\UsuariosService" id="uifi.integrantes.usuarios">
          <argument type="service" id="service_container" />
      </service>

      <service class="UIFI\IntegrantesBundle\Service\GruposService" id="uifi.integrantes.grupos">
          <argument type="service" id="service_container" />
      </service>
    </services>

</container>
