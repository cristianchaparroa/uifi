<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>

        <service class="UIFI\GrupLACScraperBundle\Service\Getinformacion\GetInformacion" id="uifi.gruplac.scrap.getinfo">
            <argument type="service" id="service_container" />
        </service>

        <service class="UIFI\GrupLACScraperBundle\Service\GruplacService" id="uifi.gruplac.scrap.gruplac">
            <argument type="service" id="service_container" />
        </service>

        <service class="UIFI\GrupLACScraperBundle\Service\ImportarRevistasService" id="uifi.gruplac.importar.revistas">
            <argument type="service" id="service_container" />
        </service>

        <!-- servicios de Assamblers -->
        <service class="UIFI\GrupLACScraperBundle\Assembler\ArticuloAssembler" id="uifi.gruplac.assembler.articulo">
          <argument type="service" id="service_container" />
        </service>
        <service class="UIFI\GrupLACScraperBundle\Assembler\GrupoAssembler" id="uifi.gruplac.assembler.grupo">
            <argument type="service" id="service_container" />
        </service>
        <service class="UIFI\GrupLACScraperBundle\Assembler\IntegranteAssembler" id="uifi.gruplac.assembler.integrante">
          <argument type="service" id="service_container" />
        </service>

        <!-- Servicios de scrapping -->
        <service class="UIFI\GrupLACScraperBundle\Service\Scraper\ScraperService" id="uifi.gruplac.scrap.scraper">
            <argument type="service" id="service_container" />
        </service>

        <service class="UIFI\GrupLACScraperBundle\Service\Scraper\ArticulosScraperService" id="uifi.gruplac.service.scraper.articulo">
        </service>

        <service class="UIFI\GrupLACScraperBundle\Service\Scraper\GrupoScraperService" id="uifi.gruplac.service.scraper.grupo">
        </service>

        <service class="UIFI\GrupLACScraperBundle\Service\Scraper\IntegrantesScraperService" id="uifi.gruplac.service.scraper.integrantes">
        </service>
        <!-- Utilidades -->

        <service class="UIFI\GrupLACScraperBundle\Util\GrupoCollectionUtilities" id="uifi.gruplac.util.collection.grupo">
        </service>





    </services>
</container>
